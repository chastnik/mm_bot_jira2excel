# Mattermost настройки
MATTERMOST_URL=https://your-mattermost-instance.com
MATTERMOST_TOKEN=your_bot_token_here
MATTERMOST_TEAM_ID=your_team_id
MATTERMOST_SSL_VERIFY=true
MATTERMOST_USE_WEBSOCKET=true

# Jira настройки (только URL, учетные данные запрашиваются у каждого пользователя)
JIRA_URL=https://your-company.atlassian.net

# Настройки бота
BOT_NAME=jira-timesheet-bot
LOG_LEVEL=INFO 